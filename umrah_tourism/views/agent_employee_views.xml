<odoo>
  <record id="view_umrah_agent_employee_tree" model="ir.ui.view">
    <field name="name">umrah.agent.employee.tree</field>
    <field name="model">umrah.agent.employee</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="agent_id"/>
        <field name="email"/>
        <field name="mobile"/>
        <field name="permission"/>
        <field name="user_id"/>
        <field name="active"/>
      </tree>
    </field>
  </record>
  <record id="view_umrah_agent_employee_form" model="ir.ui.view">
    <field name="name">umrah.agent.employee.form</field>
    <field name="model">umrah.agent.employee</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="agent_id"/>
            <field name="email"/>
            <field name="mobile"/>
            <field name="permission"/>
            <field name="user_id"/>
            <field name="active"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
