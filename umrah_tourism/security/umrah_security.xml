<odoo>
    <data noupdate="1">
        <record id="group_umrah_manager" model="res.groups">
            <field name="name">Umrah Manager</field>
            <field name="category_id" ref="base.module_category_services_management"/>
        </record>
        <record id="group_umrah_employee" model="res.groups">
            <field name="name">Umrah Employee</field>
            <field name="category_id" ref="base.module_category_services_management"/>
            <field name="implied_ids" eval="[(4, ref('group_umrah_agent'))]"/>
        </record>
        <record id="group_umrah_agent" model="res.groups">
            <field name="name">Umrah Agent</field>
            <field name="category_id" ref="base.module_category_services_management"/>
        </record>
        <record id="group_umrah_supervisor" model="res.groups">
            <field name="name">Umrah Supervisor</field>
            <field name="category_id" ref="base.module_category_services_management"/>
            <field name="implied_ids" eval="[(4, ref('group_umrah_employee'))]"/>
        </record>
    </data>
</odoo>
